{
  "name": "cacheSimulator",
  "private": true,
  "version": "0.0.1",
  "description": "Cache Simulator",
  "productName": "cache_simulator",
  "main": "main.js",
  "engines": {
    "node": "6.0.0",
    "npm": "3.8.9"
  },
  "scripts": {
    "build": "webpack --watch",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "start": "NODE_ENV=dev node src/server/util/startup.js --history-api-fallback",
    "start_prod": "NODE_ENV=production node start src/server/util/startup.js --node-args='history-api-fallback'",
    "start_electron": "electron main.js",
    "build_electron": "electron-packager . --all",
    "test": "jest --coverage"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/enzyme",
      "<rootDir>/node_modules/immutable"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Limmen/cache_simulator"
  },
  "homepage": "http://www.cachesimulator.com/",
  "keywords": [
    "Cache",
    "Simulator"
  ],
  "author": {
    "name": "Kim Hammar",
    "email": "kimham@kth.se",
    "url": "https://github.com/Limmen"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-jest": "^12.0.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "electron-packager": "^7.1.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.2.0",
    "eslint-plugin-react": "^4.0.0",
    "jest-cli": "^12.0.2",
    "mocha": "^2.4.5",
    "open": "0.0.5",
    "react-addons-test-utils": "^0.14.0",
    "sinon": "^1.17.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0"
  },
  "dependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.15.1",
    "colors": "^1.1.2",
    "compression": "^1.6.1",
    "core-js": "^2.0.0",
    "ejs": "^2.4.1",
    "electron-prebuilt": "^1.2.5",
    "es6-module-loader": "^0.17.11",
    "expect": "^1.19.0",
    "express": "^4.13.4",
    "express-history-api-fallback": "^2.0.0",
    "file-loader": "^0.8.5",
    "fixed-data-table": "^0.6.0",
    "history": "^2.1.1",
    "immutable": "^3.8.1",
    "morgan": "^1.7.0",
    "node-jsx": "^0.13.3",
    "react": "^0.14.0",
    "react-addons-css-transition-group": "^15.1.0",
    "react-dimensions": "^1.1.1",
    "react-dom": "^0.14.8",
    "react-highlight": "^0.8.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "react-scroll": "^1.0.17",
